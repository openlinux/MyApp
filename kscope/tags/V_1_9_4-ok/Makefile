MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p
INSTALL_FILE= 
INSTALL_DIR = 
SUBTARGETS =	 \
		sub-core \
		sub-cscope \
		sub-editor \
		sub-app

first: all

all: Makefile $(SUBTARGETS)

core/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "core" || $(MKDIR) "core"
	cd core && $(QMAKE) core.pro -o $(MAKEFILE)
sub-core: core/$(MAKEFILE) FORCE
	cd core && $(MAKE) -f $(MAKEFILE)

cscope/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "cscope" || $(MKDIR) "cscope"
	cd cscope && $(QMAKE) cscope.pro -o $(MAKEFILE)
sub-cscope: cscope/$(MAKEFILE) FORCE
	cd cscope && $(MAKE) -f $(MAKEFILE)

editor/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "editor" || $(MKDIR) "editor"
	cd editor && $(QMAKE) editor.pro -o $(MAKEFILE)
sub-editor: editor/$(MAKEFILE) FORCE
	cd editor && $(MAKE) -f $(MAKEFILE)

app/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "app" || $(MKDIR) "app"
	cd app && $(QMAKE) app.pro -o $(MAKEFILE)
sub-app: app/$(MAKEFILE) FORCE
	cd app && $(MAKE) -f $(MAKEFILE)

Makefile: kscope.pro  /usr/share/qt3/mkspecs/default/qmake.conf config
	$(QMAKE) -o Makefile kscope.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile kscope.pro

all: $(SUBTARGETS)
qmake_all: core/$(MAKEFILE) cscope/$(MAKEFILE) editor/$(MAKEFILE) app/$(MAKEFILE)
	 ( [ -d core ] && cd core ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
