	 ( [ -d app ] && cd app ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uicables mocables uiclean mocclean lexclean yaccclean : qmake_all FORCE
	 ( [ -d core ] && cd core ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d cscope ] && cd cscope ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d editor ] && cd editor ; $(MAKE) -f $(MAKEFILE) $@; ) || true
