	 ( [ -d app ] && cd app ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
install_subdirs: qmake_all FORCE
	 ( [ -d core ] && cd core ; $(MAKE) -f $(MAKEFILE) install; ) || true
	 ( [ -d cscope ] && cd cscope ; $(MAKE) -f $(MAKEFILE) install; ) || true
	 ( [ -d editor ] && cd editor ; $(MAKE) -f $(MAKEFILE) install; ) || true
	 ( [ -d app ] && cd app ; $(MAKE) -f $(MAKEFILE) install; ) || true
distclean: qmake_all FORCE
	 ( [ -d core ] && cd core ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d cscope ] && cd cscope ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d editor ] && cd editor ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d app ] && cd app ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

